<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".LifeStyleApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LifeStyle"
        tools:targetApi="31">
        <activity
            android:name=".jour4.bottom_navigation.ui.list.ImageDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".jour4.bottom_navigation.BottomNavigationActivity"
            android:exported="true"
            android:label="@string/title_activity_bootom_navigation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".jour3.viewpager.ViewpagerActivity"
            android:exported="true" />
        <activity
            android:name=".jour3.navigationdrawer.NaviagtionDrawerActivity"
            android:exported="false"
            android:label="@string/title_activity_naviagtion_drawer"
            android:theme="@style/Theme.LifeStyle.NoActionBar" />
        <activity
            android:name=".jour3.list_habitude.HabitudeActivity"
            android:exported="true" />
        <activity
            android:name=".jour2.ContactMaterialActivity"
            android:exported="true" />
        <activity
            android:name=".jour2.ContactActivity"
            android:exported="false" />
        <activity
            android:name=".CatActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.LifeStyle.NoActionBar" />
    </application>

</manifest>